{
    "name": "journi-web-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build:app && npm run build:storybook",
        "build:app": "GIT_SHA=`git rev-parse HEAD` react-scripts build",
        "build:storybook": "STORYBOOK_CURRENT_SOURCE_CODE=`git rev-parse HEAD` build-storybook -s ./public -o ./styleguide",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "start": "react-scripts start",
        "storybook": "start-storybook -p 9009 -s public",
        "svg": "make svg",
        "test": "npm run test:acceptance && CI=true npm run test:visual",
        "test:acceptance": "NODE_ENV=test npx cucumber-js -f node_modules/cucumber-pretty || : ",
        "test:visual": "react-scripts test"
    },
    "dependencies": {
        "@apollo/react-hooks": "3.1.4",
        "apollo-boost": "0.4.7",
        "graphql": "15.0.0",
        "i18next-browser-languagedetector": "4.0.2",
        "i18next": "19.3.2",
        "react-dom": "16.12.0",
        "react-helmet": "6.0.0",
        "react-i18next": "11.3.3",
        "react-router-dom": "5.1.2",
        "react": "16.12.0",
        "styled-components": "5.1.0",
        "voca": "1.4.0"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "5.3.18",
        "@storybook/addon-actions": "5.3.18",
        "@storybook/addon-links": "5.3.18",
        "@storybook/addon-storyshots": "5.3.18",
        "@storybook/addon-storyshots-puppeteer": "5.3.18",
        "@storybook/addons": "5.3.18",
        "@storybook/preset-create-react-app": "2.1.1",
        "@storybook/react": "5.3.18",
        "@svgr/cli": "4.1.0",
        "@testing-library/jest-dom": "4.2.4",
        "@testing-library/react": "9.3.2",
        "@testing-library/user-event": "7.1.2",
        "@types/jest": "24.0.0",
        "@types/node": "12.0.0",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@types/react-helmet": "5.0.15",
        "@types/react-router-dom": "5.1.3",
        "@types/styled-components": "5.0.1",
        "@types/voca": "1.4.0",
        "babel-plugin-macros": "2.8.0",
        "chai": "4.2.0",
        "create-index": "2.5.0",
        "cucumber": "6.0.5",
        "cucumber-pretty": "6",
        "node-sass": "4.13.1",
        "portfinder": "^1.0.25",
        "prettier": "1.19.1",
        "puppeteer": "2.1.1",
        "react-scripts": "3.4.1",
        "react-test-renderer": "16.13.1",
        "require-context.macro": "1.2.2",
        "serve-handler": "^6.1.2",
        "tslint": "5.20.1",
        "tslint-config-prettier": "1.18.0",
        "tslint-immutable": "6.0.1",
        "tslint-loader": "3.5.4",
        "tslint-plugin-prettier": "2.1.0",
        "tslint-react": "4.1.0",
        "typescript": "~3.7.2"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "homepage": ".",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "https://charli-app-service.dev.janusplatform.io",
    "prettier": {
        "bracketSpacing": true,
        "printWidth": 80,
        "semi": false,
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "all"
    }
}
